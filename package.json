  "scripts": {
    "migrations:build": "rimraf migrations-build && tsc -p tsconfig.migrations.json",
    "migrations:run": "npm run typeorm -- migration:run",
    "migrations:test:run": "DB_NAME=postgresql://ddhub:ddhub@localhost:5432/ddhub_test npm run typeorm -- migration:run",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "generate:envs": "npm run generate:api:envs && npm run generate:scheduler:envs",
    "generate:api:envs": "npm run build:api:envs && node dist/apps/dsb-client-gateway-api-envs/main.js",
    "generate:scheduler:envs": "npm run build:scheduler:envs && node dist/apps/dsb-client-gateway-scheduler-envs/main.js",
    "build:scheduler:envs": "nx build-envs dsb-client-gateway-scheduler",
    "build:api:envs": "nx build-envs dsb-client-gateway-api",
    "test:spec:clients": "nx test ddhub-client-gateway-clients",
    "commit": "git-cz",
    "start": "nx serve",
    "start:prod": "npm run migrations:run && node dist/apps/dsb-client-gateway-api/main.js",
    "start:ui": "wait-on tcp:3333 && nx serve",
    "start:api": "nx serve dsb-client-gateway-api",
    "build:scheduler": "nx build dsb-client-gateway-scheduler",
    "start:dev": "concurrently \"npm run start:api\" \"npm run start:ui\"",
    "build": "npm run build:client && nx build",
    "build:client": "nx run dsb-client-gateway-api-client:build-all",
    "export": "npm run build:client && nx export dsb-client-gateway-frontend",
    "build:api": "nx build dsb-client-gateway-api",
    "test": "nx test",
    "test:spec:api": "nx test dsb-client-gateway-api",
    "test:e2e:api": "nx test dsb-client-gateway-api-e2e",
    "prepare": "husky install"
  },
